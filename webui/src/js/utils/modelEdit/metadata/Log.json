          },
          {
            "type": "attributes",
            "labelKey": "miscellaneous-attributes-label",
            "attributes": [
              "TriggerTruncationStackFrameTriggerDepth",
              "TriggerTruncationStackFrameDepthAfterTrigger"
            ],
            "addRemainingAttributes": true
          }
        ]
      },
      {
        "type": "hidden",
        "attributes": [
          "Log4jLoggingEnabled",
          "MemoryBufferSeverity",
          "MemoryBufferSize",
          "MemoryBufferFilter",
          "ServerLoggingBridgeAtRootLoggerEnabled",
          "ServerLoggingBridgeUseParentLoggersEnabled"
