{
  "UnixMachine": {
    "summaryAttributes": {
      "NodeManager/NMType": {},
      "NodeManager/ListenAddress": {},
      "NodeManager/ListenPort": {}
    },
    "attributeDetails": {
      "NMSocketCreateTimeoutInMillis": {
        "disabled": "nodeManagerShellTypeFields",
        "validators": [ "range(0,)" ]
      },
      "NMType": {
        "optionsMatch": "lower",
        "options": [
          { "value": "plain", "labelKey": "options-plain-label" },
          { "value": "ssl", "label": "SSL" },
          { "value": "rsh", "label": "RSH" },
          { "value": "ssh", "label": "SSH" }
        ]
      },
      "NodeManagerHome": {
        "disabled": "nodeManagerShellTypeFields",
        "fileOptions": [ { "type": "dir" } ]
      },
      "PasswordEncrypted": {
        "disabled": "nodeManagerShellTypeFields"
      },
      "PostBindGID": {
        "disabled": "unixNodeManagerPostBindGIDFields"
      },
      "PostBindUID": {
        "disabled": "unixNodeManagerPostBindUIDFields"
      },
      "ShellCommand": {
        "disabled": "nodeManagerShellTypeFields",
        "usesPath": false
      },
      "UserName": {
        "disabled": "nodeManagerShellTypeFields"
      }
    },
    "sections": [
      {
        "type": "attributes",
        "attributes": [
          "PostBindUIDEnabled",
          "PostBindUID",
          "PostBindGIDEnabled",
          "PostBindGID",
          "Notes"
        ]
      },
      {
        "type": "hidden",
        "attributes": [
          "Address"
        ],
        "folders": [
          "NodeManager"
        ]
      }
    ]
  },
  "UnixMachine/NodeManager": {
    "sections": [
      {
        "type": "attributes",
        "attributes": [
          "NMType",
          "DebugEnabled",
          "ListenAddress",
          "ListenPort",
          "ShellCommand",
          "NodeManagerHome",
          "UserName",
          "PasswordEncrypted",
          "NMSocketCreateTimeoutInMillis",
          "Notes"
        ]
      },
      {
        "type": "attributesCollapsible",
        "labelKey": "advanced-label",
        "addRemainingAttributes": true
      },
      {
        "type": "hidden",
        "attributes": [
          "Adapter",
          "AdapterName",
          "AdapterVersion"
        ]
      }
    ]
  }
}