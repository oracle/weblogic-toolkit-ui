{
  "JDBCStore": {
    "summaryAttributes": {
      "PrefixName": {},
      "DataSource": {},
      "Target": {}
    },
    "attributeDetails": {
      "ConnectionCachingPolicy": {
        "options": [
          { "value": "DEFAULT", "labelKey": "options-default-label" },
          { "value": "MINIMAL", "labelKey": "options-minimal-label" },
          { "value": "NONE", "labelKey": "options-none-label" }
        ]
      },
      "CreateTableDDLFile": {
        "archiveTypes": ["script"]
      },
      "DataSource": {
        "editorType": "combo",
        "optionsMethod": "getDataSourceOptions"
      },
      "DeletesPerBatchMaximum": {
        "validators": [ "range(1,100)" ]
      },
      "DeletesPerStatementMaximum": {
        "validators": [ "range(1,100)" ]
      },
      "DistributionPolicy": {
        "options": [
          { "value": "Distributed", "labelKey": "store-distribution-policy-distributed-label" },
          { "value": "Singleton", "labelKey": "store-distribution-policy-singleton-label" }
        ]
      },
      "FailbackDelaySeconds": {
        "validators": [ "range(-1,)" ]
      },
      "FailOverLimit": {
        "validators": [ "range(-1,)" ]
      },
      "InitialBootDelaySeconds": {
        "validators": [ "range(0,)" ]
      },
      "InsertsPerBatchMaximum": {
        "validators": [ "range(1,100)" ]
      },
      "MigrationPolicy": {
        "options": [
          { "value": "Off", "labelKey": "options-off-label" },
          { "value": "On-Failure", "labelKey": "store-migration-policy-on-failure-label" },
          { "value": "Always", "labelKey": "options-always-label" }
        ]
      },
      "NumberOfRestartAttempts": {
        "disabled": "persistentStoreDisableRestartFields",
        "validators": [ "range(-1,)" ]
      },
      "PartialClusterStabilityDelaySeconds": {
        "validators": [ "range(0,)" ]
      },
      "ReconnectRetryIntervalMillis": {
        "validators": [ "range(100,10000)" ]
      },
      "ReconnectRetryPeriodMillis": {
        "validators": [ "range(200,300000)" ]
      },
      "SecondsBetweenRestarts": {
        "disabled": "persistentStoreDisableRestartFields",
        "validators": [ "range(1,)" ]
      },
      "ThreeStepThreshold": {
        "validators": [ "range(4000,)" ]
      },
      "WorkerCount": {
        "validators": [ "range(1,1000)" ]
      },
      "WorkerPreferredBatchSize": {
        "validators": [ "range(1,)" ]
      }
    },
    "sections": [
      {
        "type": "tab",
        "labelKey": "general-tab-label",
        "sections": [
          {
            "type": "attributes",
            "attributes": [
              "PrefixName",
              "DataSource",
              "DeploymentOrder",
              "Notes",
              "Target"
            ]
          },
          {
            "type": "attributesCollapsible",
            "labelKey": "advanced-label",
            "attributes": [
              "LogicalName",
              "XAResourceName",
              "CreateTableDDLFile"
            ],
            "addRemainingAttributes": true
          },
          {
            "type": "hidden",
            "attributes": [
              "ConnectionPool"
            ]
          }
        ]
      },
      {
        "type": "attributesTab",
        "labelKey": "high-availability-tab-label",
        "attributes": [
          "DistributionPolicy",
          "MigrationPolicy",
          "RestartInPlace",
          "RebalanceEnabled",
          "NumberOfRestartAttempts",
          "SecondsBetweenRestarts",
          "InitialBootDelaySeconds",
          "FailbackDelaySeconds",
          "PartialClusterStabilityDelaySeconds",
          "FailOverLimit",
          "ReconnectRetryIntervalMillis",
          "ReconnectRetryPeriodMillis"
        ]
      },
      {
        "type": "attributesTab",
        "labelKey": "tuning-tab-label",
        "attributes": [
            "ConnectionCachingPolicy",
            "OraclePiggybackCommitEnabled",
            "DeletesPerBatchMaximum",
            "InsertsPerBatchMaximum",
            "DeletesPerStatementMaximum",
            "ThreeStepThreshold",
            "WorkerCount",
            "WorkerPreferredBatchSize"
        ]
      }
    ]
  }
}