{
  "Machine": {
    "mergeFolder": "NodeManager",
    "summaryAttributes": {
      "NodeManager/NMType": {},
      "NodeManager/ListenAddress": {},
      "NodeManager/ListenPort": {}
    },
    "attributeDetails": {
      "NMSocketCreateTimeoutInMillis": {
        "disabled": "nodeManagerShellTypeFields",
        "validators": [ "range(0,)" ]
      },
      "NMType": {
        "optionsMatch": "lower",
        "options": [
          { "value": "plain", "labelKey": "options-plain-label" },
          { "value": "ssl", "label": "SSL" },
          { "value": "rsh", "label": "RSH" },
          { "value": "ssh", "label": "SSH" }
        ]
      },
      "NodeManagerHome": {
        "disabled": "nodeManagerShellTypeFields",
        "fileOptions": [ { "type": "dir" } ]
      },
      "PasswordEncrypted": {
        "disabled": "nodeManagerShellTypeFields"
      },
      "ShellCommand": {
        "disabled": "nodeManagerShellTypeFields",
        "usesPath": false
      },
      "UserName": {
        "disabled": "nodeManagerShellTypeFields"
      }
    },
    "sections": [
      {
        "type": "tab",
        "labelKey": "general-tab-label",
        "sections": [
          {
            "type": "attributes",
            "attributes": [
              "Notes"
            ]
          }
        ]
      },
      {
        "type": "hidden",
        "attributes": [
          "Address",
          "InstalledVMMAdapter",
          "Adapter",
          "AdapterName",
          "AdapterVersion"
        ]
      },
      {
        "type": "tab",
        "labelKey": "node-manager-tab-label",
        "sections": [
          {
            "type": "attributes",
            "attributes": [
              "NMType",
              "DebugEnabled",
              "ListenAddress",
              "ListenPort",
              "ShellCommand",
              "NodeManagerHome",
              "UserName",
              "PasswordEncrypted",
              "NMSocketCreateTimeoutInMillis"
            ]
          },
          {
            "type": "attributesCollapsible",
            "labelKey": "advanced-label",
            "addRemainingAttributes": true
          }
        ]
      }
    ]
  }
}