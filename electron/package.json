{
  "name": "wktui",
  "productName": "WebLogic Kubernetes Toolkit UI",
  "version": "2.0.0-beta.0",
  "description": "WebLogic Kubernetes Toolkit UI",
  "copyright": "Copyright (c) 2021, 2025, Oracle and/or its affiliates.",
  "homepage": "https://github.com/oracle/weblogic-toolkit-ui",
  "main": "app/main.js",
  "scripts": {
    "start": "electron --trace-warnings . dev",
    "install-tools": "node --trace-uncaught scripts/installTools.js",
    "update-alias-labels": "node --trace-uncaught scripts/updateAliasLabels.js",
    "build": "npm run build:jet && npm run install-tools && npm run build:installer",
    "build:jet": "cd ../webui && npm run build:release && cd ../electron",
    "build:installer": "electron-builder -p never",
    "eslint": "npx eslint app/main.js app/js/*.js app/js/prompt/*.js app/test/*.js",
    "test": "mocha --timeout 10000 --reporter spec app/test/*-test.js",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:oracle/weblogic-toolkit-ui.git"
  },
  "keywords": [
    "weblogic",
    "kubernetes"
  ],
  "author": {
    "name": "Oracle",
    "email": "info@oracle.com"
  },
  "contributors": [
    {
      "name": "Richard Killen",
      "email": "richard.killen@oracle.com"
    },
    {
      "name": "Russell Gold",
      "email": "russell.gold@oracle.com"
    },
    {
      "name": "Johnny Shum",
      "email": "johnny.shum@oracle.com"
    },
    {
      "name": "Robert Patrick",
      "email": "robert.patrick@oracle.com"
    }
  ],
  "license": "UPL-1.0",
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "electron": "^39.2.5",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "mocha": "^11.7.5",
    "nyc": "^17.1.0",
    "proxyquire": "^2.1.3"
  },
  "dependencies": {
    "electron-updater": "^6.6.2",
    "extract-zip": "^2.0.1",
    "gunzip-maybe": "^1.4.2",
    "https-proxy-agent": "^7.0.6",
    "i18next": "^25.7.1",
    "i18next-fs-backend": "^2.6.1",
    "js-yaml": "^4.1.1",
    "jszip": "^3.10.1",
    "node-fetch": "^2.7.0",
    "tar": "^7.5.2",
    "uuid": "^13.0.0",
    "which": "^6.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "build": {
    "appId": "com.oracle.weblogic.wktui",
    "asar": true,
    "afterSign": "./build/notarize.js",
    "files": [
      "!app/web/test",
      "!app/test"
    ],
    "electronLanguages": [
      "en",
      "de",
      "es",
      "it",
      "fr",
      "ja",
      "ko",
      "pt-BR",
      "zh-CN",
      "zh-TW"
    ],
    "extraFiles": [
      {
        "from": "../tools",
        "to": "tools"
      }
    ],
    "extraResources": [
      {
        "from": "../LICENSE.txt",
        "to": "LICENSE.txt"
      },
      {
        "from": "../THIRD_PARTY_LICENSES.txt",
        "to": "THIRD_PARTY_LICENSES.txt"
      },
      {
        "from": "../WKTUI_VERSION.txt",
        "to": "WKTUI_VERSION.txt"
      },
      {
        "from": "../url-catalog.json",
        "to": "url-catalog.json"
      }
    ],
    "fileAssociations": [
      {
        "ext": "wktproj",
        "name": "WKTPROJ",
        "description": "WebLogic Kubernetes Toolkit project file",
        "mimeType": "application/vnd.oracle.wkt.project",
        "role": "Editor"
      }
    ],
    "directories": {
      "output": "../dist"
    },
    "mac": {
      "category": "Utility",
      "target": {
        "target": "default"
      },
      "darkModeSupport": false,
      "type": "distribution",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./build/entitlements.mac.plist"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "category": "Utility",
      "target": [
        {
          "target": "AppImage"
        },
        {
          "target": "rpm"
        },
        {
          "target": "deb"
        }
      ]
    },
    "rpm": {
      "depends": [
        "libXScrnSaver",
        "libsecret-devel",
        "libxshmfence",
        "libdrm",
        "libgbm",
        "alsa-lib",
        "xauth",
        "atk-devel",
        "java-atk-wrapper"
      ],
      "fpm": [
        "--rpm-rpmbuild-define",
        "_build_id_links none"
      ]
    },
    "deb": {
      "depends": [
        "gconf2",
        "gconf-service",
        "libnotify4",
        "libappindicator1",
        "libxtst6",
        "libnss3",
        "libsecret-1-dev"
      ]
    }
  }
}
